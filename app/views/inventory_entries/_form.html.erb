<% form_for(@inventory_entry) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :inventory_transaction %><br />
    <%= f.text_field :inventory_transaction %>
  </div>
  <div class="field">
    <%= f.label :added_date %><br />
    <%= f.datetime_select :added_date %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
