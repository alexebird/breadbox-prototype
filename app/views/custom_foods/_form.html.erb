<% form_for(@custom_food) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :fresh_for %><br />
    <%= f.text_field :fresh_for %>
  </div>
  <div class="field">
    <%= f.label :storage_location %><br />
    <%= f.text_field :storage_location %>
  </div>
  <div class="field">
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </div>
  <div class="field">
    <%= f.label :provided_food %><br />
    <%= f.text_field :provided_food %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
