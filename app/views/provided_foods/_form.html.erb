<% form_for(@provided_food) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :fresh_for %><br />
    <%= f.text_field :fresh_for %>
  </div>
  <div class="field">
    <%= f.label :storage_location %><br />
    <%= f.text_field :storage_location %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
